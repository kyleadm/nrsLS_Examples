[OCCA]
backend = serial

[GENERAL]
polynomialOrder = 7
stopAt = endTime
endTime = 1e-3
dt = 1e-04
timeStepper = tombo2

checkPointControl = simulationTime 
checkPointInterval = 0.2

[PROBLEMTYPE]
equation = navierStokes

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
solver = none
boundaryTypeMap = O 
residualTol = 1e-06
density = 1.0
viscosity = 1e-10 

[SCALAR00]
boundaryTypeMap = I
residualTol = 1e-08
rho = 1.0
diffusivity = 1e-10 
