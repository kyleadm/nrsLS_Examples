[GENERAL]
polynomialOrder = 7
stopAt = numSteps
numSteps = 0
dt = 5e-02
timeStepper = tombo2

checkPointControl = simulationTime 
checkPointInterval = 50

[PROBLEMTYPE]
equation = navierStokes

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
solver = none
boundaryTypeMap = O 
residualTol = 1e-06
density = 1.0
viscosity = 1e-10 

[SCALAR00]
boundaryTypeMap = I
residualTol = 1e-08
rho = 1.0
diffusivity = 1e-10 
